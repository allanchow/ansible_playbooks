[Unit]
Description={{ service_name }}

[Timer]
OnCalendar=*-*-* *:*:0/{{ heartbeat_every_sec }}
RandomizedDelaySec=0
AccuracySec=1us
RemainAfterElapse=off
Persistent=false
Unit=heartbeat.service

[Install]
WantedBy=timers.target
